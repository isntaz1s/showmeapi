{
  "name": "showmeapi",
  "version": "1.0.0",
  "description": "A REST API application which contains of some api endpoints",
  "author": "isntaz1s",
  "license": "MIT",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "install": "npm i",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.ts",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\" --fix"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/env": "^5.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/passport": "^3.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@neondatabase/serverless": "^0.10.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^12.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  }
}
